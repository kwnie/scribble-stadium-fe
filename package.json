{
  "name": "story-squad",
  "version": "0.1.0",
  "engines": {
    "node": "14.x"
  },
  "private": false,
  "license": "MIT",
  "repository": "github:Lambda-School-Labs/labs-spa-starter",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@auth0/auth0-react": "^1.8.0",
    "@craco/craco": "^5.9.0",
    "ant-design-pro": "^2.3.2",
    "antd": "^4.16.13",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "craco": "0.0.3",
    "craco-less": "^1.20.0",
    "emailjs": "^3.6.0",
    "emailjs-com": "^2.6.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "gsap": "^3.8.0",
    "less": "^4.1.1",
    "prettier": "^2.4.1",
    "react": "^16.14.0",
    "react-datepicker": "^3.8.0",
    "react-dom": "^16.14.0",
    "react-h5-audio-player": "^3.7.2",
    "react-image-picker": "^1.1.1",
    "react-pdf": "^5.4.0",
    "react-pin-input": "^1.0.1",
    "react-plotly.js": "^2.5.1",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^3.4.4",
    "react-sizeme": "^2.6.12",
    "react-spring": "^8.0.27",
    "react-test-renderer": "^16.14.0",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.1"
  },
  "scripts": {
    "dev-server": "webpack-dev-server --open",
    "heroku-postbuild": "npm run build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "craco test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "coverage": "npm test -- --coverage --watchAll=false",
    "lint": "eslint 'src/**/*.{js,jsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css,scss}'",
    "storybook": "start-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "update": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier-eslint --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/stories/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/api/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^7.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-prop-type-error": "^1.1.0",
    "lint-staged": "^10.5.4",
    "prettier-eslint-cli": "^5.0.1"
  }
}
